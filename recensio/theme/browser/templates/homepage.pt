<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone">


<body>
<metal:main fill-slot="main">

        <div id="content_left">
        <div id="column1">
            <div class="box">
              <h2 i18n:translate="label_latest_review_journals">Neueste Rezensionsteile online</h2>
              <div id="latest_internet_resources_box" tal:define="results view/getReviewJournals">
				<ul tal:repeat="res results">
					<li><span tal:replace="res/Title" /> in <span tal:replace="res/publication_title" /></li>
				</ul>
			  </div>
            </div>
            <div class="box">
              <h2 i18n:translate="label_latest_reviews">Neueste Rezensionen online</h2>
              <div id="latest_reviews_box" tal:define="results view/getReviewMonographs">
                <span tal:repeat="langres results">
        		    <label><span tal:replace="langres/langname" /></label>
                    <ul tal:repeat="res langres/results">
                        <li><span tal:replace="res/Title" /></li>
                    </ul>
                </span>
              </div>
            </div>
        </div>
        <div id="column2">
            <div class="box">
              <h2 i18n:translate="label_latest_monographies">Neueste Präsentationen von Monographien und Aufsätze</h2>
              <div id="latest_monographies_box" tal:define="results view/getPrintedPresentations">
				<ul tal:repeat="res results">
					<li><span tal:replace="res/Title" /></li>
				</ul>
			  </div>
            </div>
            <div class="box">
              <h2 i18n:translate="label_latest_internet_resources">Neueste Präsentationen von Internetressourcen</h2>
              <div id="latest_internet_resources_box" tal:define="results view/getOnlinePresentations">
				<ul tal:repeat="res results">
					<li><span tal:replace="res/Title" /></li>
				</ul>
			  </div>
            </div>
        </div>
        <div style="clear:left;"></div>
        <div id="column_wide">
            Teilehmende Zeitschriften
        </div>
        </div>
        



</metal:main>
</body>
</html>
